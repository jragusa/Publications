issn = {1527-1404},
journal = {Journal of Sedimentary Research},
keywords = {grain-size,mica,settling velocity,sieving},
mendeley-tags = {grain-size,mica,settling velocity,sieving},
number = {4},
pages = {1379--1391},
title = {{Grain-Size Analyses of Mica Within Sediments and the Hydraulic Equivalence of Mica and Quartz}},
url = {http://jsedres.sepmonline.org/cgi/doi/10.1306/212F85E4-2B24-11D7-8648000102C1865D http://archives.datapages.com/data/sepm/journals/v51-54/data/054/054004/1379.htm},
volume = {54},
year = {1984}
}
@article{Korsch1993,
abstract = {Pronounced changes in chemical composition were detected upwards within single turbidite beds with SiO2 contents deceasing by as much as 20{\%}. All other major elements and eighteen trace elements increase in abundance upwards. Enrichment factors from bed base to top range from 1.2 (Na2O) to 5.7 (Cu). These changes are related to textural and mineralogical grading, that in turn were the result of turbidity current action before and during the deposition of the bed. We found only slight changes in the modal petrographic composition of the sandstone fractions, although this technique is operator-dependent and only semi-quantitative in nature. Chemical analysis appears to be a more sensitive, more rapid and less subjective method of determining provenance and the compositional contrasts within beds. Chemical anomalies produced by dilution effects from quartz overgrowths and intrabasinal material, however, show the need for utilisation of a combination of techniques, and for an appreciation of the interdependence of texture, framework modes and bulk chemistry. The chemical variations we detect in single beds also reflect the variation in a suite of representative samples collected from within a confined (several km2) geographic area. Hence, either sampling over a given area or specific sampling of individual beds (provided that the bed is typical of the succession as a whole) can be used to determine the compositional variations within a succession as a whole.},
author = {Korsch, R.J. and Roser, B.P. and Kamprad, J.L.},
