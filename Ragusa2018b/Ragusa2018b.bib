@article{Zuffa1991,
abstract = {When we determine framework petrography of deep-sea arenites for provenance studies we should concentrate not only on what particles make up sedimentary beds but also when, where and how they form. Taking such information into account, grains can be classified according to their temporal, spatial and compositional characteristics. This will allow a better understanding of palaeogeography and palaeotectonics of the basin/source system. Two examples from the literature that support this approach are reported.},
author = {Zuffa, Gian Gaspare},
doi = {10.1144/GSL.SP.1991.057.01.03},
issn = {0305-8719},
journal = {Geological Society, London, Special Publications},
