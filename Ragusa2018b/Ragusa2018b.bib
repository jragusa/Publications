keywords = {Carpathian flysch,compositional variation,grain-size,provenance},
mendeley-tags = {Carpathian flysch,compositional variation,grain-size,provenance},
number = {1},
pages = {44--56},
title = {{Textural and Mineralogical Sorting Relationships in Krosno Formation (Oligocene) Turbidites, Polish Carpathian Mountains}},
url = {http://jsedres.sepmonline.org/cgi/doi/10.1306/212F6CBC-2B24-11D7-8648000102C1865D http://archives.datapages.com/data/sepm/journals/v42-46/data/045/045001/0044.htm?doi=10.1306{\%}2F212F6CBC-2B24-11D7-8648000102C1865D},
volume = {45},
