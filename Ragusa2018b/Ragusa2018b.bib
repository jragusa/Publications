title = {{La Nappe Sup{\'{e}}rieure des Pr{\'{e}}alpes: subdivisions et principaux caract{\`{e}}res du sommet de l'{\'{e}}difice pr{\'{e}}alpin}},
url = {http://retro.seals.ch/digbib/view2?pid=egh-001:1972:65::66},
