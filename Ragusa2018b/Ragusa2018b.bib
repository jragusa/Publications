mendeley-tags = {clay minerals,compositional variation,geochemistry,grain-size,provenance,trace elements},
month = {feb},
number = {1-2},
pages = {15--35},
title = {{Geochemical, petrographic and grain-size variations within single turbidite beds}},
url = {http://linkinghub.elsevier.com/retrieve/pii/003707389390180D},
volume = {83},
year = {1993}
}
