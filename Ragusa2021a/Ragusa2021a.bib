journal = {Eclogae Geologicae Helvetiae},
keywords = {Alps,Collisional orogen,Geophysical transects,Gurnigel nappe,Paleogeography,Structural geology,Tectonic map,Tectonics,Tectono-metamorphic evolution,Valais},
mendeley-tags = {Gurnigel nappe,Valais},
number = {1},
pages = {99--101},
pmid = {21343385},
title = {{Reply to Comment by W. Kurz on Tectonic map and overall architecture of the Alpine orogen}},
